{
  "name": "@chain-registry/interfaces",
  "version": "0.31.0",
  "description": "Chain Registry interfaces",
  "author": "Dan Lynch <pyramation@gmail.com>",
  "homepage": "https://github.com/cosmology-tech/chain-registry",
  "license": "SEE LICENSE IN LICENSE",
  "main": "index.js",
  "module": "esm/index.js",
  "types": "index.d.ts",
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cosmology-tech/chain-registry"
  },
  "bugs": {
    "url": "https://github.com/cosmology-tech/chain-registry/issues"
  },
  "scripts": {
    "copy": "copyfiles -f LICENSE README.md package.json dist",
    "clean": "del dist/**",
    "prepare": "npm run build",
    "types": "chain-registry codegen --outputDir ./src/ --no-camelCase --registryDir ../../packages/chain-registry/chain-registry",
    "build": "npm run clean; tsc; tsc -p tsconfig.esm.json; npm run copy",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "devDependencies": {
    "@chain-registry/cli": "^1.32.0"
  },
  "keywords": [
    "chain-registry",
    "web3",
    "cosmos",
    "interchain"
  ]
}
